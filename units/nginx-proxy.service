[Unit]
Description=Nginx proxy server
After=docker.service
After=transmission.service
Wants=transmission.service
Requires=docker.service

[Service]
TimeoutStartSec=0
ExecStartPre=-/usr/bin/docker kill nginx-proxy
ExecStartPre=-/usr/bin/docker rm nginx-proxy
#ExecStartPre=/usr/bin/docker pull nginx-proxy
ExecStart=/usr/bin/docker run --name nginx-proxy -p 80:80 -p 443:443 --link transmission:seedbox eatnumber1/nginx-proxy
ExecStop=/usr/bin/docker stop nginx-proxy
